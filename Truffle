/// required to define ETH_BIGNUMBER_ROUNDING_MODE
var BigNumber = __webpack_require__(147);

var ETH_UNITS = [
    'wei',
    'kwei',
    'Mwei',
    'Gwei',
    'szabo',
    'finney',
    'femtoether',
